{
  "version": 2,
  "name": "cads-research-visualization",
  "description": "Interactive visualization of CADS research publications and themes",
  "public": true,
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "visuals/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/data/(.*\\.gz)",
      "dest": "/visuals/public/data/$1",
      "headers": {
        "Content-Encoding": "gzip",
        "Content-Type": "application/json",
        "Cache-Control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/data/(.*)",
      "dest": "/visuals/public/data/$1",
      "headers": {
        "Content-Type": "application/json",
        "Cache-Control": "public, max-age=86400",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/app\\.(js|min\\.js)",
      "dest": "/visuals/public/$1",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/(.*\\.(css|js|html|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/visuals/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/",
      "dest": "/visuals/public/index.html",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/visuals/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/data/(.*\\.gz)",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "gzip"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/data/(.*\\.json)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        },
        {
          "key": "Content-Type",
          "value": "text/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/visuals/public/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "framework": null,
  "installCommand": null,
  "buildCommand": null,
  "devCommand": null,
  "outputDirectory": "visuals/public"
}